<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dreamscloud.deneme">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.dreamscloud.deneme.OnlinePlayers"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dreamscloud.deneme.Login"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dreamscloud.deneme.Introduction"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dreamscloud.deneme.OfflineOyun"
            android:screenOrientation="portrait" />
        <activity
            android:screenOrientation="portrait"
            android:name="com.dreamscloud.deneme.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name="com.dreamscloud.deneme.OnlineOyun"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dreamscloud.deneme.Firebase"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dreamscloud.deneme.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>